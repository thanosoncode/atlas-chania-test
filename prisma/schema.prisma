generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  image     String
  content   String
  createdAt DateTime @default(now())
  likes     Int
}

model Wod {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  wodTitle     String
  wodContent   String
  wodPlainText String
  createdAt    DateTime @default(now())
}
